{
  "name": "get-a-life-alert",
  "version": "1.0.0",
  "description": "Monitor network time usage and send SMS alerts via Twilio",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "bin": {
    "get-a-life-alert": "./dist/cli.js"
  },
  "keywords": [
    "network-monitoring",
    "parental-controls",
    "sms-alerts",
    "fritz-router",
    "twilio"
  ],
  "author": "ranguard",
  "license": "MIT",
  "dependencies": {
    "twilio": "^4.19.0",
    "dotenv": "^16.3.1",
    "js-yaml": "^4.1.0",
    "node-cron": "^3.0.3",
    "commander": "^11.1.0",
    "axios": "^1.6.0",
    "better-sqlite3": "^9.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/js-yaml": "^4.0.9",
    "@types/better-sqlite3": "^7.6.8",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}